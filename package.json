{
  "name": "peak-member-management-api",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "A node js with typescript project template using fastify for api.",
  "main": "src/app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./dist/app.js",
    "build": "tsc -p ./tsconfig.json",
    "dev": "nodemon --exec ts-node ./src/app.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "npx typeorm-ts-node-commonjs schema:sync",
    "typeorm:cache": "npx typeorm-ts-node-commonjs cache:clear",
    "schema:drop": "npx typeorm-ts-node-commonjs -d ./src/dal/db/db-source.ts",
    "migration:create": "typeorm migration:create ./src/dal/migrations/migration",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/dal/migrations/migration -d ./src/dal/db/db-source.ts",
    "migration:show": "npx typeorm-ts-node-commonjs migration:show -d ./src/dal/db/db-source.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d  ./src/dal/db/db-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/dal/db/db-source.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.4.0",
    "cron": "^3.1.7",
    "cron-parser": "^4.9.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.6.3",
    "nodemailer": "^6.9.14",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.7.5",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/cron": "^2.4.0",
    "@types/handlebars": "^4.1.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.4",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.14.8",
    "@types/nodemailer": "^6.4.15",
    "@types/reflect-metadata": "^0.1.0",
    "@types/socket.io": "^3.0.2",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2"
  }
}
